parameter_defaults:
  ContainerImagePrepare:
  - push_destination: true
    excludes:
      - ceph
      - ose-prometheus
      - ceilometer
    set:
      name_prefix: openstack-
      name_suffix: ''
      namespace: ${OPENSTACK_CONTAINER_REGISTRY}/rhosp-rhel8
      neutron_driver: ovn
      rhel_containers: false
      tag: '16.0'
    tag_from_label: '{version}'
  ${rhsm_image_registry_credentials}
